# Environment Variables Template
# Copy this file to .env.local and fill in your values

# Firebase Configuration
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id

# DODO Payments
VITE_DODO_API_KEY=your_dodo_api_key
VITE_DODO_WEBHOOK_SECRET=your_dodo_webhook_secret

# Analytics & Tracking
VITE_GA_MEASUREMENT_ID=G-XXXXXXXXXX
VITE_FB_PIXEL_ID=your_facebook_pixel_id

# Development
VITE_NODE_ENV=development
VITE_DEBUG=true

# Production URLs
VITE_PRODUCTION_URL=https://buildfy.droppybuilder.com
VITE_API_URL=https://buildfy.droppybuilder.com/api
