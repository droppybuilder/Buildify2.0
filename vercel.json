{
  "functions": {
    "api/**/*.js": {
      "maxDuration": 30
    }
  },  "rewrites": [
    {
      "source": "/api/payment/create",
      "destination": "/api/payment/create.js"
    },
    {
      "source": "/api/webhooks/dodo",
      "destination": "/api/webhooks/dodo.js"
    },
    {
      "source": "/((?!api).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://buildfyweb.vercel.app"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization, webhook-id, webhook-signature, webhook-timestamp"
        }
      ]
    }
  ]
}
